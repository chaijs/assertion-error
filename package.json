{
    "name": "assertion-error",
    "version": "1.1.0",
    "description": "Error constructor for test and validation frameworks that implements standardized AssertionError specification.",
    "author": "Jake Luer <jake@qualiancy.com> (http://qualiancy.com)",
    "license": "MIT",
    "types": "./dist/index.d.ts",
    "keywords": [
        "test",
        "assertion",
        "assertion-error"
    ],
    "repository": {
        "type": "git",
        "url": "git@github.com:chaijs/assertion-error.git"
    },
    "engines": {
        "node": ">=12"
    },
    "files": [
        "dist"
    ],
    "type": "module",
    "module": "./dist/index.js",
    "main": "./dist/index.js",
    "scripts": {
        "build": "tsc",
        "pretest": "npm run build",
        "test": "npm run test:node && npm run test:typescript",
        "test:node": "NODE_ENV=test node ./test/index.js",
        "test:typescript": "tsc test/typings.ts dist/index.d.ts --noEmit"
    },
    "devDependencies": {
        "typescript": "^4.4.3"
    }
}
